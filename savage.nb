Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"diagonalizeH", "[", 
    RowBox[{"paritySector_", ",", " ", "fname_String"}], "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"FileExistsQ", "[", "fname", "]"}], ",", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{"If", " ", "file", " ", "exists"}], ",", " ", 
       RowBox[{"load", " ", "it"}]}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"hMatrix", " ", "=", 
      RowBox[{"Import", "[", "fname", "]"}]}], ",", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"Else", ",", " ", 
       RowBox[{
        RowBox[{"compute", " ", "it"}], "..."}]}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Block", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"hOnParitySector", ",", " ", "len", ",", " ", "ctr"}], "}"}],
         ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"hOnParitySector", " ", "=", " ", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"H", "/.", 
              RowBox[{
               RowBox[{"Ket", "[", "\[CapitalPsi]", "]"}], "\[Rule]", 
               "basisKet"}]}], "//.", "rules"}], ",", 
            RowBox[{"{", 
             RowBox[{"basisKet", ",", "paritySector"}], "}"}]}], "]"}]}], ";",
          "\[IndentingNewLine]", 
         RowBox[{"len", "=", 
          RowBox[{"Length", "[", "paritySector", "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"L", ">", "2"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"ctr", " ", "=", " ", "0"}], ";", "\[IndentingNewLine]", 
            RowBox[{"SetSharedVariable", "[", "ctr", "]"}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"Monitor", "[", 
             RowBox[{
              RowBox[{"hMatrix", " ", "=", " ", 
               RowBox[{"SparseArray", "[", "\[IndentingNewLine]", 
                RowBox[{"ParallelTable", "[", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"ctr", "++"}], ";", 
                   RowBox[{
                    RowBox[{
                    RowBox[{
                    RowBox[{
                    RowBox[{
                    "hOnParitySector", "\[LeftDoubleBracket]", "i", 
                    "\[RightDoubleBracket]"}], ".", 
                    RowBox[{
                    "paritySector", "\[LeftDoubleBracket]", "j", 
                    "\[RightDoubleBracket]"}]}], "/.", "params"}], "//.", 
                    "rules"}], "//", "Expand"}]}], ",", "\[IndentingNewLine]", 
                  RowBox[{"{", 
                   RowBox[{"i", ",", " ", "len"}], "}"}], ",", " ", 
                  RowBox[{"{", 
                   RowBox[{"j", ",", "len"}], "}"}]}], "]"}], "]"}]}], ",", 
              RowBox[{"ProgressIndicator", "[", 
               RowBox[{"ctr", ",", " ", 
                RowBox[{"{", 
                 RowBox[{"1", ",", " ", 
                  SuperscriptBox["len", "2"]}], "}"}]}], "]"}]}], "]"}]}], 
           ",", "\[IndentingNewLine]", 
           RowBox[{"hMatrix", " ", "=", " ", 
            RowBox[{"SparseArray", "[", 
             RowBox[{"Table", "[", 
              RowBox[{
               RowBox[{
                RowBox[{
                 RowBox[{
                  RowBox[{
                   RowBox[{
                   "hOnParitySector", "\[LeftDoubleBracket]", "i", 
                    "\[RightDoubleBracket]"}], ".", 
                   RowBox[{
                   "paritySector", "\[LeftDoubleBracket]", "j", 
                    "\[RightDoubleBracket]"}]}], "/.", "params"}], "//.", 
                 "rules"}], "//", "Expand"}], ",", 
               RowBox[{"{", 
                RowBox[{"i", ",", " ", "len"}], "}"}], ",", " ", 
               RowBox[{"{", 
                RowBox[{"j", ",", "len"}], "}"}]}], "]"}], "]"}]}]}], 
          "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{"(*", " ", 
          RowBox[{"...", " ", "and", " ", "export", " ", "it"}], " ", "*)"}], 
         "\[IndentingNewLine]", 
         RowBox[{"Export", "[", 
          RowBox[{"fname", ",", " ", "hMatrix", ",", " ", "\"\<MTX\>\""}], 
          "]"}], ";"}]}], "\[IndentingNewLine]", "]"}], ";"}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.765553133165941*^9, 3.765553372979718*^9}, {
  3.76555342753831*^9, 3.765553497220106*^9}, {3.7655535466740847`*^9, 
