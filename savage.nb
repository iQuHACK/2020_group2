            RowBox[{"srcTerm", ".", "srcTerm"}], "//.", "rules"}], "]"}], "//.",
           "rules"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"srcTerm", " ", "=", " ", 
         RowBox[{"srcTerm", " ", "/", "srcNorm"}]}], ";", 
        "\[IndentingNewLine]", 
