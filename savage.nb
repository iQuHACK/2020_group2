Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"findOvlap", "[", 
    RowBox[{
    "fromState_", ",", " ", "opSrc_", ",", " ", "opSnk_", ",", " ", 
     "toState_"}], "]"}], ":=", "\[IndentingNewLine]", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"!", "excludeSlow"}], ",", "\[IndentingNewLine]", 
     RowBox[{"Block", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "srcOvlap", ",", " ", "snkOvlap", ",", " ", "srcTerm", ",", "snkNorm",
          ",", " ", "srcNorm", ",", " ", "snkTerm"}], "}"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Print", "[", "\"\<Computing the source term...\>\"", "]"}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"srcTerm", " ", "=", " ", 
         RowBox[{"findAction", "[", 
          RowBox[{"opSrc", ",", " ", "fromState"}], "]"}]}], ";", 
